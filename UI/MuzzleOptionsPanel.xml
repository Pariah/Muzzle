<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="MuzzleOptionsPanelTemplate" virtual="true">
        <Size x="800" y="600"/>
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
            <BackgroundInsets>
                <AbsInset left="11" right="12" top="12" bottom="11"/>
            </BackgroundInsets>
            <TileSize>
                <AbsValue val="32"/>
            </TileSize>
            <EdgeSize>
                <AbsValue val="32"/>
            </EdgeSize>
        </Backdrop>
        <Frames>
            <Frame name="$parentTitleBar">
                <Size x="0" y="40"/>
                <Anchors>
                    <Anchor point="TOPLEFT"/>
                    <Anchor point="TOPRIGHT"/>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString name="$parentTitle" inherits="GameFontNormalLarge" text="Muzzle Options">
                            <Anchors>
                                <Anchor point="LEFT" x="16" y="0"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
            <ScrollFrame name="$parentScrollFrame" inherits="UIPanelScrollFrameTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="16" y="-40"/>
                    <Anchor point="BOTTOMRIGHT" x="-32" y="16"/>
                </Anchors>
                <ScrollChild>
                    <Frame name="$parentContent">
                        <Size x="760" y="544"/>
                    </Frame>
                </ScrollChild>
            </ScrollFrame>
        </Frames>
    </Frame>
</Ui>